    "url": "https://github.com/mbkkong/webpack-laravel-mix/issues"
  },
  "homepage": "https://github.com/mbkkong/webpack-laravel-mix#readme",
  "dependencies": {
    "laravel-mix": "^5.0.0"
  },
  "devDependencies": {
    "bootstrap": "^4.3.1",
    "cross-env": "^6.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.12",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10"
  }
