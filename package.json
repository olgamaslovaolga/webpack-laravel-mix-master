{
  "name": "webpack-laravel-mix",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "yarn development",
    "development": "npx cross-env NODE_ENV=development npx webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "yarn development --watch",
    "hot": "npx cross-env NODE_ENV=development npx webpack-dev-server --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "yarn production",
    "production": "npx cross-env NODE_ENV=production npx webpack --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mbkkong/webpack-laravel-mix.git"
  },
